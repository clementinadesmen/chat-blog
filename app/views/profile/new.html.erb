<h1 align="center">Creer votre profil</h1>
<%= form_for(@profile, :html => { class: "form-horizontal", role: "form" }) do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>
      <%= f.label :bio %>
      <%= f.text_field :bio %>
      
      <%= f.submit "Enregistrer mon profil", class: "btn btn-primary" %>
<% end %>